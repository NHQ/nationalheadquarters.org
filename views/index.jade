div.container

  div.console
    div#reel
      div.buffer
      iframe(id='film',src="http://player.vimeo.com/video/40337000?byline=0&amp;color=ffffff", frameborder="0", webkitAllowFullScreen mozallowfullscreen allowFullScreen)
    br
    div.humans#humans
      div.buffer
      h1.subHead HUMANS
      each person, key in people
        div(class='person')
          img(class='swap', src='images/'+key+'_hi_res_thumb.jpg')
          p !{person.blurb}
    div#news
      div.buffer
      h1.subHead NEWS
      p.news !{news}
